@model List<BurgerMenuProject.Entities.Contact>
<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">İletişim Bilgileri</h4>
            <p class="card-description">
            </p>
            <div class="table-responsive">
                <table class="table">
                    <thead>
                        <tr>
                            <th>Görsel</th>
                            <th>E-Posta Adresi</th>
                            <th>Telefon Numarası</th>
                            <th>Adres</th>
                            <th>Description</th>
                            <th>Güncelle</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                    <tr>
                        <td><img src="@item.Image"/></td>
                        <td>@item.Email</td>
                        <td>@item.PhoneNumber</td>
                        <td>@item.Address</td>
                        <td>@item.Description</td>
                        <td><a href="/Admin/Settings/UpdateContact/@item.Id" class="btn btn-success">Güncelle</a></td>
                    </tr>
                }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>